<!DOCTYPE html>
<html>

<head>
    <title>TSA Simulator</title>

    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>

    <script type="text/javascript" src="../js/game.js"></script>

</head>
<link rel="stylesheet" type="text/css" href="../styles/style_game.css">

<body>
    <div id="box1" class="box boxclicked">
    </div>

    <div id="book_conatianer" clicked="false">
        <div id="event_page" class="show_content_top">
            <div class="form_holder_top">
                <div class="buffer_left">
                    <div id=event_title></div>
                    <div id="event_image_holder">event_image_holder</div>
                    <div id=event_content></div>
                    <div id=score></div>
                </div>
            </div>
        </div>

        <div id="cube" class="inactive cube hide">
            <div class="cube_inner">
                <div class="cube_front" ready="true"></div>
                <div class="page_left"></div>
                <div class="page_right">
                    <div id=front_page class="show_content_right" clicked="true">
                        <div id="form_holder1" class="form_holder form_inactive">
                            <div class="buffer">
                                <div class="image_holder">IMAGE HOLDER</div>
                                <div class="line_holder">
                                    <p class="holder_text">The name is </p>
                                    <div id="passport_firstname1" class="passport_slot">Test</div>
                                    <div id="passport_lastname1" class="passport_slot">Test</div>
                                </div>
                                <div class="line_holder">
                                    <p class="holder_text"> from </p>
                                    <div id="passport_nationality1" class="passport_slot"></div>
                                    <p class="holder_text"> of </p>
                                    <div id="passport_culture1" class="passport_slot"></div>
                                </div>
                                <div class="line_holder">
                                    <p class="holder_text">I am a</p>
                                    <div id="passport_race1" class="passport_slot"></div>
                                    <div id="passport_religion1" class="passport_slot"></div>
                                </div>
                                <div class="line_holder">
                                    <p class="holder_text">working as</p>
                                    <div id="passport_occupation1" class="passport_slot"></div>
                                </div>
                                <div class="line_holder">
                                    <p class="holder_text">carrying</p>
                                    <div id="passport_property1" class="passport_slot"></div>
                                    <p class="holder_text">solidi with me</p>
                                </div>
                            </div>
                        </div>
                        <p class="label unselectable"></p>
                    </div>
                    <div id=back_page class="default_content_right" clicked="false">
                        <div class="form_holder">
                            <div class="buffer">
                                <div class="image_holder">IMAGE HOLDER</div>
                                <div class="line_holder">
                                    <p class="holder_text">The name is </p>
                                    <div id="passport_firstname2" class="passport_slot">Test</div>
                                    <div id="passport_lastname2" class="passport_slot">Test</div>
                                </div>
                                <div class="line_holder">
                                    <p class="holder_text"> from </p>
                                    <div id="passport_nationality2" class="passport_slot"></div>
                                    <p class="holder_text"> of </p>
                                    <div id="passport_culture2" class="passport_slot"></div>
                                </div>
                                <div class="line_holder">
                                    <p class="holder_text">I am a</p>
                                    <div id="passport_race2" class="passport_slot"></div>
                                    <div id="passport_religion2" class="passport_slot"></div>
                                </div>
                                <div class="line_holder">
                                    <p class="holder_text">working as</p>
                                    <div id="passport_occupation2" class="passport_slot"></div>
                                </div>
                                <div class="line_holder">
                                    <p class="holder_text">carrying</p>
                                    <div id="passport_property2" class="passport_slot"></div>
                                    <p class="holder_text">solidi with me</p>
                                </div>
                            </div>
                        </div>
                        <p class="label unselectable"></p>
                    </div>
                    <div id="button_container" class="button_container" clicked="false">
                        <div id="approve" class="binary_selection_approve">Admit</div>
                        <div id="deny" class="binary_selection_deny">Deny</div>
                    </div>

                    <div id="button_container2" class="button_container2" clicked="false">
                        <div id="continue" class="binary_selection_continue">Continue</div>
                        <div id="end" class="binary_selection_end">End</div>
                    </div>
                </div>
                <div class="cube_back"></div>
                <div class="cube_left"></div>
                <div class="cube_right"></div>
                <div class="cube_top"></div>
                <div class="cube_bottom"></div>
            </div>
        </div>
    </div>
    </div>
</body>

</html>